<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0E0C0C"
    tools:context=".EndFragment">


    <VideoView
        android:id="@+id/simpleVideoView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/t"
        android:layout_marginTop="@dimen/_300sdp"
        android:layout_marginLeft="@dimen/_180sdp"
        android:gravity="right"
        android:layout_width="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/white"
        android:fontFamily="@font/bold"
        android:layout_height="wrap_content"
        android:text="@string/ahsant" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/_300sdp"
        android:layout_height="106dp"
        android:layout_marginLeft="@dimen/_1sdp"
        android:layout_marginTop="@dimen/_335sdp"
        android:gravity="right"
        android:fontFamily="@font/bold"
        android:text="@string/aa"
        android:textColor="@color/white"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       android:layout_gravity="bottom"
        android:fontFamily="@font/bold"
        android:gravity="right"
        android:text="✨✨✨✨✨✨✨✨✨✨✨✨✨"
        android:textColor="@color/white"
        android:textSize="22dp" />

    </FrameLayout>